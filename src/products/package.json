{
  "name": "@adobe/commerce-cif-commercetools-product",
  "version": "1.2.0",
  "description": "Adobe Commerce Integration Framework (CIF REST) Commercetools Integration Product Project",
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/commerce-cif-commercetools.git"
  },
  "dependencies": {
    "@commercetools/sdk-client": "1.4.1",
    "@adobe/commerce-cif-commercetools-common": "1.0.2",
    "@adobe/commerce-cif-common": "1.0.0",
    "@adobe/commerce-cif-model": "1.1.0"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-plugin-header": "2.0",
    "js-yaml": "3.9.0",
    "serverless": "1.40.0",
    "serverless-openwhisk": "0.18.3",
    "serverless-webpack": "4.3.0",
    "webpack": "4.0.1"
  },
  "eslintConfig": {
    "extends": "../../.eslintrc.js"
  },
  "scripts": {
    "fix-serverless-permission": "chmod +x $(npm bin)/serverless",
    "lint": "eslint -f table --ignore-path ../../.eslintignore .",
    "deploy": "serverless deploy",
    "remove": "serverless remove",
    "deploy-current": "npm run deploy -- --ow-package commerce-cif-commercetools-product@$npm_package_version --shared",
    "deploy-latest": "npm run deploy -- --ow-package commerce-cif-commercetools-product@latest --shared",
    "deploy-suffix": "npm run deploy -- --ow-package commerce-cif-commercetools-product@${OW_PACKAGE_SUFFIX} --shared",
    "deploy-package": "npm run deploy-current; npm run deploy-latest",
    "remove-current": "npm run remove -- --ow-package commerce-cif-commercetools-product@$npm_package_version",
    "remove-latest": "npm run remove -- --ow-package commerce-cif-commercetools-product@latest",
    "remove-suffix": "npm run remove -- --ow-package commerce-cif-commercetools-product@${OW_PACKAGE_SUFFIX}",
    "remove-package": "npm run remove-current; npm run remove-latest"
  }
}
